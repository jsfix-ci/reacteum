{
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 5.2.0"
  },
  "name": "reacteum",
  "version": "2.0.0",
  "description": "Boilerplate for React JS",
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "webpack",
    "reacteum"
  ],
  "main": "n/a",
  "scripts": {
    "dev": "npm run dev:client",
    "dev:client": "reacteum develop",
    "dev:server": "reacteum serve",
    "dev:story": "npx -p storybook start-storybook -p 6006",
    "build": "npm run build:server",
    "build:client": "reacteum bundle",
    "build:server": "reacteum transpile",
    "build:story": "npx -p storybook build-storybook",
    "lint": "eslint src/**/*.js || true",
    "format": "prettier-eslint --eslint-config-path ./.eslintrc.js --write",
    "test": "jest --bail",
    "check": "npm run lint && npm t",
    "precommit": "lint-staged"
  },
  "author": "wonism <yocee57@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonism/reacteum.git"
  },
