{
  "name": "reacteum",
  "version": "1.0.2",
  "description": "Boilerplate for React JS",
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "redux",
    "redux-saga",
    "webpack",
    "reacteum"
  ],
  "main": "n/a",
  "scripts": {
    "dev:client": "NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js --hot --host 0.0.0.0",
    "dev:server": "npm run build:client && nodemon server --exec babel-node",
    "build:client": "NODE_ENV=production webpack -p --config config/webpack.config.prod.js",
    "build:server": "npm run build:client && NODE_ENV=production webpack -p --config config/webpack.config.server.js",
    "lint": "eslint src/**/*.js || true",
    "format": "prettier-eslint --write",
    "test": "jest --bail",
    "precommit": "lint-staged"
  },
  "author": "wonism <yocee57@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonism/reacteum.git"
  },
  "bugs": {
    "url": "https://github.com/wonism/reacteum/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "prop-types": "^15.0.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "emotion": "^9.1.3",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "morgan": "^1.9.0",
    "prop-types": "^15.6.1",
    "qs": "^6.5.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-emotion": "^9.1.3",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-emotion": "^9.1.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^0.14.3",
    "jest": "^23.0.1",
    "jsonlint": "^1.6.3",
    "lint-staged": "^7.1.2",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.4",
    "prettier-eslint-cli": "^4.7.1",
    "redux-mock-store": "^1.5.1",
    "webpack": "^4.9.1",
    "webpack-assets-manifest": "^3.0.1",
    "webpack-cli": "^2.1.4",
