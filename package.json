  },
  "name": "reacteum",
  "version": "1.2.0",
  "description": "Boilerplate for React JS",
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "redux",
    "redux-saga",
    "webpack",
    "reacteum"
